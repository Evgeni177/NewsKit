{
  "name": "newskit",
  "version": "0.4.0",
  "description": "A React Component library implementing the News design language",
  "keywords": [
    "react",
    "newsuk"
  ],
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "",
  "scripts": {
    "dev": "concurrently --names \"src,storybook,docs\" \"yarn build:ts --watch\" \"yarn dev:storybook\" \"next site -p 8081\"",
    "dev:docs": "concurrently --names \"src,docs\" \"yarn build:ts --watch\" \"next site -p 8081\"",
    "dev:storybook": "start-storybook --ci -p 6006",
    "lint:code:base": "eslint --report-unused-disable-directives --ext=.js,.ts,.jsx,.tsx --cache",
    "lint:code": "scripts/assert-tests-folder.js && yarn lint:code:base .",
    "lint:code:fix": "yarn lint:code --fix",
    "lint:markdown": "markdownlint -c ./.markdownlint.json *.md **/*.md",
    "lint": "yarn lint:code && yarn lint:markdown",
    "test:unit": "jest --coverage --coverageReporters=lcov --coverageReporters=text",
    "test:unit:comps": "yarn test:unit --projects=src",
    "test:unit:site": "yarn test:unit --projects=site",
    "test:unit:ci": "yarn test:unit --runInBand",
    "test": "yarn lint && yarn test:unit",
    "test:ci": "yarn lint && yarn test:unit:ci",
    "prebuild": "rm -rf dist && rm -rf dist-storybook && rm -rf public",
    "build:ts": "NODE_ENV=production tsc",
    "build:ts:esm": "NODE_ENV=production tsc -m es2015 --outDir dist/esm",
    "build:copy-files": "cp README_COMPONENTS.md dist/README.md; cp .npmignore dist/.npmignore",
    "build:package-json": "node ./scripts/build-package-json.js",
    "build:comps": "yarn build:ts && yarn build:ts:esm && yarn build:copy-files && yarn build:package-json",
    "build:storybook": "build-storybook -c .storybook -o dist-storybook",
    "build:docs": "next build site && next export site -o public",
    "build": "yarn build:comps && yarn build:storybook && yarn build:docs",
    "e2e:build": "webpack --config webpack.e2e.config.js --mode=production",
    "e2e:serve": "http-server e2e/dist -p 8080",
    "e2e:dev": "cypress open",
    "e2e:test": "TZ=UTC cypress run --spec '**/*.spec.js'",
    "e2e:comps": "TZ=UTC cypress run --spec 'cypress/components/**/*'",
    "e2e:docs": "TZ=UTC cypress run --spec 'cypress/site/**/*'",
    "e2e:docker": "concurrently -k -s first -n \"serve,build\" \"yarn e2e:serve \" \"yarn e2e:build && node cypress/support/wait-for-server.js $cypress_DOC_SITE_URL && yarn e2e:test\"",
    "docker:docs": "rm -rf public && yarn build:comps && yarn build:docs && yarn serve:docs",
    "prepare": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "serve:docs": "http-server public -p 8081",
    "serve:storybook": "http-server dist-storybook -p 6006",
    "dependencies:check": "package-change-checker --install-cmd=\"yarn install --pure-lockfile\" HEAD@{1} HEAD",
    "create:release": "sh ./scripts/release.sh"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@emotion/serialize": "^0.11.9",
    "@mapbox/rehype-prism": "^0.3.1",
    "@mdx-js/tag": "^0.20.0",
    "@storybook/react": "^4.0.0",
    "@testing-library/react": "^9.1.1",
    "@types/amphtml-validator": "^1.0.0",
    "@types/card-validator": "^4.1.0",
    "@types/date-fns": "^2.6.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.15",
    "@types/jest-matcher-utils": "^21.0.2",
    "@types/jscodeshift": "^0.6.1",
    "@types/just-extend": "^1.1.0",
    "@types/memoize-one": "^4.1.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "10",
    "@types/popper.js": "^1.11.0",
    "@types/prettier": "^1.16.4",
    "@types/react": "16.9.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-input-mask": "^2.0.1",
    "@types/react-is": "^16.7.1",
    "@types/react-syntax-highlighter": "^10.2.1",
    "@types/react-virtualized": "^9.21.2",
    "@types/recursive-readdir": "^2.2.0",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/storybook__react": "^4.0.2",
    "@types/tar": "^4.0.2",
    "@types/typescript": "^2.0.0",
    "@types/webpack": "^4.4.34",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "amphtml-validator": "^1.0.28",
    "axe-core": "^3.1.2",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "csstype": "2",
    "cypress": "3.4.0",
    "cypress-axe": "^0.5.1",
    "cypress-image-snapshot": "^3.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "http-server": "^0.11.1",
    "husky": "^2.7.0",
    "import-glob": "^1.5.0",
    "jest": "^24.0.0",
    "jest-dom": "^4.0.0",
    "jest-emotion": "^10.0.14",
    "jest-matcher-utils": "^24.0.0",
    "jscodeshift": "^0.6.4",
    "lint-staged": "^9.1.0",
    "markdownlint-cli": "^0.17.0",
    "mdast-util-to-string": "^1.0.7",
    "mkdirp": "^0.5.1",
    "next": "^9.0.0",
    "next-images": "^1.1.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "package-change-checker": "^1.2.4",
    "prettier": "^1.16.0",
    "pretty-proptypes": "^0.6.0",
    "react": "^16.9.0",
    "react-codesandboxer": "^3.1.1",
    "react-dom": "^16.9.0",
    "react-markdown": "^4.0.3",
    "react-syntax-highlighter": "^10.1.2",
    "recursive-readdir": "^2.2.2",
    "tar": "^4.4.8",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "unist-builder": "^2.0.2",
    "unist-util-find": "^1.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "date-fns": "^2.6.0",
    "dotenv": "^8.0.0",
    "emotion": "^10.0.14",
    "emotion-theming": "^10.0.14",
    "isomorphic-fetch": "^2.2.1"
  },
  "resolutions": {
    "react": "16.9.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test",
      "post-merge": "yarn dependencies:check",
      "post-checkout": "yarn dependencies:check"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:code:base --fix",
      "git add"
    ]
  }
}
